 } from 'react';\n\nconst url = 'https://www.thecocktaildb.com/api/json/v1/1/search.php?s=';\nconst AppContext = React.createContext();\n\n\nconst AppProvider = ({ children }) => {\n  const [loading, setLoading] = useState(false);\n  const [searchVal, setSearchVal] = useState('');\n  const [cocktails, setCocktails] = useState([]);\n\n  const updateSearch = (e) => {\n    setSearchVal(e.target.value);\n  }\n\n  const fetchData = async () => {\n    setLoading(true);\n    const res = await fetch(url+searchVal);\n    const data = await res.json();\n    if (!data.drinks) {\n      setCocktails([]);\n    }\n    else {\n      const drinkList = data.drinks.map((drink) => \n      drink =  {\n        id: drink['idDrink'],\n        name: drink['strDrink'],\n        glass: drink['strGlass'],\n        alcohol: drink['strAlcoholic'],\n        img: drink['strDrinkThumb']\n      })\n      setCocktails(drinkList);\n    }\n    setLoading(false);\n  }\n\n  useEffect(() => {\n    fetchData();\n  }, [searchVal])\n\n  return (\n    <AppContext.Provider value={{ loading, cocktails, updateSearch }}>\n      {children}\n    </AppContext.Provider>\n  )\n}\n\nexport const useGlobalContext = () => {\n  return useContext(AppContext);\n}\nexport { AppContext, AppProvider };","/Users/taeng9/Desktop/project/React/react-projects/00-envi/src/components/navbar.js",[],"/Users/taeng9/Desktop/project/React/react-projects/00-envi/src/components/search.js",[],"/Users/taeng9/Desktop/project/React/react-projects/00-envi/src/components/cocktail-list.js",[],"/Users/taeng9/Desktop/project/React/react-projects/00-envi/src/components/cocktail.js",[],"/Users/taeng9/Desktop/project/React/react-projects/00-envi/src/pages/home.js",[],"/Users/taeng9/Desktop/project/React/react-projects/00-envi/src/pages/about.js",[],"/Users/taeng9/Desktop/project/React/react-projects/00-envi/src/components/aboutinfo.js",[],{"ruleId":"57","replacedBy":"58"},{"ruleId":"59","replacedBy":"60"},{"ruleId":"61","severity":1,"message":"62","lin